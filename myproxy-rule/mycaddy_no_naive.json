{
    "admin": {
        "disabled": true,
        "config": {
            "persist": false
        }
    },
    "logging": {
        "logs": {
            "default": {
                "writer": {
                    "output": "file",
                    "filename": "/home/zwyyy/log/caddy/default.log"
                },
                "encoder": {
                    "format": "console"
                },
                "level": "WARN"
            }
        }
    },
    "apps": {
        "http": {
            "servers": {
                "srvh1": {
                    "listen": [
                        ":80"
                    ],
                    "routes": [
                        {
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "headers": {
                                        "Location": [
                                            "https://{http.request.host}{http.request.uri}"
                                        ]
                                    },
                                    "status_code": 301
                                }
                            ]
                        }
                    ],
                    "protocols": [
                        "h1"
                    ]
                },
                "srvh2": {
                    "listen": [
                        "127.0.0.1:7443"
                    ],
                    "listener_wrappers": [
                        {
                            "wrapper": "proxy_protocol"
                        },
                        {
                            "wrapper": "tls"
                        }
                    ],
                    "routes": [
                        {
                            "handle": [
                                {
                                    "handler": "forward_proxy",
                                    "auth_user_deprecated": "zwyyy456",
                                    "auth_pass_deprecated": "naive_1136.",
                                    "hide_ip": true,
                                    "hide_via": true,
                                    "probe_resistance": {}
                                }
                            ]
                        },
                        {
                            "handle": [
                                {
                                    "handler": "headers",
                                    "response": {
                                        "set": {
                                            "Strict-Transport-Security": [
                                                "max-age=31536000; includeSubDomains; preload"
                                            ]
                                        }
                                    }
                                },
                                {
                                    "handler": "file_server",
                                    "root": "/home/zwyyy/webpage"
                                }
                            ]
                        }
                    ],
                    "tls_connection_policies": [
                        {
                            "match": {
                                "sni": [
                                    "naive-ccs.ggll.eu.org"
                                ]
                            },
                            "protocol_min": "tls1.2",
                            "protocol_max": "tls1.2",
                            "cipher_suites": [
                                "TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256"
                            ],
                            "curves": [
                                "secp521r1",
                                "secp384r1",
                                "secp256r1"
                            ]
                        },
                        {
                            "match": {
                                "sni": [
                                    "ccs.ggll.eu.org"
                                ]
                            },
                            "cipher_suites": [
                                "TLS_ECDHE_ECDSA_WITH_AES_256_GCM_SHA384",
                                "TLS_ECDHE_ECDSA_WITH_AES_128_GCM_SHA256",
                                "TLS_ECDHE_ECDSA_WITH_CHACHA20_POLY1305_SHA256"
                            ],
                            "curves": [
                                "x25519",
                                "secp521r1",
                                "secp384r1",
                                "secp256r1"
                            ]
                        }
                    ],
                    "trusted_proxies": {
                        "source": "cloudflare",
                        "interval": "12h",
                        "timeout": "15s"
                    },
                    "protocols": [
                        "h1",
                        "h2"
                    ]
                }
            }
        },
        "tls": {
            "certificates": {
                "automate": [
                    "ccs.ggll.eu.org",
                    "naive-ccs.ggll.eu.org"
                ]
            },
            "automation": {
                "policies": [
                    {
                        "issuers": [
                            {
                                "module": "acme",
                                "email": "zwyyy456@gmail.com"
                            },
                            {
                                "module": "zerossl",
                                "email": "zwyyy456@gmail.com"
                            }
                        ]
                    }
                ]
            }
        }
    }
}